<%= render 'layouts/errors', :object => @food %>

<%= form_for @food, remote: true do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :quantity %>
<%= f.text_field :quantity %>
<%= f.label :calories %>
<%= f.text_field :calories %>
<%=f.hidden_field :log_id, :value => Log.find(params[:log]).id %>
<%= f.submit "Add food" %>
<% end %>
